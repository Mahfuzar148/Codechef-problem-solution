#include <iostream>
using namespace std;

int main() {
	// your code goes here
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	int t;
	cin>>t;
	while(t--)
	{
	    int n,k;
	    string s;
	    cin>>n>>k>>s;
	    if(s[0]=='0')
	    {
	        s[0]='1';
	        k--;
	    }
	    while(k--)
	    {
	        s.push_back('0');
	    }
	    cout<<s<<endl;
	}
	return 0;
}
